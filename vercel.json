{
  "version": 2,
  "rewrites": [
    {
      "source": "/blogapi/:path*",
      "destination": "https://blogapi.bloniea.com//api/open/:path*"
    },
    {
      "source": "/mycdn/:path*",
      "destination": "https://cdn.bloniea.com/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}