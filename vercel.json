{
  "version": 2,
  "rewrites": [
    {
      "source": "/blogapi/:match*",
      "destination": "https://blogapi.bloniea.ml/api/open/:match*"
    },
    {
      "source": "/mycdn/:match*",
      "destination": "https://cdn.bloniea.ml/:match*"
    },

    { "source": "/(.*)", "destination": "/" }
  ]
}
