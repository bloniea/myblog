{
  "version": 2,
  "rewrites": [
    {
      "source": "/blogapi/:match*",
      "destination": "https://blogapi.bloniea.com//api/open/:match*"
    },
    {
      "source": "/mycdn/:match*",
      "destination": "https://cdn.bloniea.com/:match*"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}